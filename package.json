{
  "name": "auto_fill",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:rebuild": "electron-rebuild -f -w better-sqlite3",
    "package": "npm run build && electron-builder",
    "gen:poster": "node scripts/generate_poster.js"
  },
  "build": {
    "appId": "com.autofill.app",
    "productName": "AutoFill",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist",
      "dist-electron"
    ],
    "extraResources": [
      {
        "from": "data",
        "to": "data",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "asar": true
  },
  "dependencies": {
    "echarts": "^6.0.0",
    "element-plus": "^2.13.1",
    "pinia": "^3.0.4",
    "playwright": "^1.58.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sass": "^1.97.3",
    "sqlite3": "^5.1.7",
    "vue": "^3.5.24",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.3",
    "@playwright/test": "^1.58.0",
    "@types/node": "^25.0.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  }
}
