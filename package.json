{
  "name": "auto_fill",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:rebuild": "electron-rebuild -f -w sqlite3",
    "package": "npm run build && electron-builder",
    "gen:poster": "node scripts/generate_poster.js"
  },
  "build": {
    "appId": "com.autofill.app",
    "productName": "AutoFill",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist",
      "dist-electron"
    ],
    "extraResources": [
      {
        "from": "data",
        "to": "data",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "asar": true
  },
  "dependencies": {
    "echarts": "^6.0.0",
    "element-plus": "^2.5.3",
    "pinia": "^2.1.7",
    "playwright": "^1.41.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sass": "^1.70.0",
    "sqlite3": "^5.1.7",
    "vue": "^3.4.15",
    "vue-i18n": "^9.9.0",
    "vue-router": "^4.2.5",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@playwright/test": "^1.41.0",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "electron": "^28.2.0",
    "electron-builder": "^24.9.1",
    "vite": "^5.1.0",
    "vite-plugin-electron": "^0.28.2",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
